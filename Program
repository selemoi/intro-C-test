#include <iostream>

int main() { 
    std::cout << "     O     O       ________" << std::endl;
    std::cout << "    /|\\   /|\\     /|      |" << std::endl;
    std::cout << "    / \\   / \\    /_|______|" << std::endl;
    std::cout << "            |   |         |" << std::endl;
    std::cout << "          --|---|         |" << std::endl;
    std::cout << "          O O   O O O O O O" << std::endl;

    std::cout << std::endl; 
    std::cout << "=============================" << std::endl;
    std::cout << "   Stick Man and Woman Holding Hands with a Car" << std::endl;
    std::cout << "===============================================" << std::endl;
    
    return 0;
}
